      "category": "Simple Lookup",
      "question": "Who scored goals for Flamengo in match M001?",
      "expected_cypher": "MATCH (p:Player)-[scored:SCORED_IN {team_id: 'T001'}]->(m:Match {match_id: 'M001'}) RETURN p.name, scored.minute, scored.goal_type",
      "expected_result": [
        {"player": "Gabriel Barbosa", "minute": 34, "type": "Open-play"},
        {"player": "Gabriel Barbosa", "minute": 67, "type": "Penalty"}
      ]
    },
    {
      "category": "Relationship Traversal",
      "question": "What teams did Neymar play for?",
      "expected_cypher": "MATCH (p:Player {player_id: 'P007'})-[r:PLAYS_FOR]->(t:Team) RETURN t.name, r.start_date, r.end_date ORDER BY r.start_date",
      "expected_result": [
        {"team": "Santos Futebol Clube", "start": "2009-03-07", "end": "2013-06-03"}
      ]
    },
    {
      "category": "Aggregation",
      "question": "Who is the top scorer among sample matches?",
      "expected_cypher": "MATCH (p:Player)-[scored:SCORED_IN]->(m:Match) RETURN p.name, count(scored) AS goals ORDER BY goals DESC LIMIT 1",
      "expected_result": [
        {"player": "Raphael Veiga", "goals": 3}
      ]
    },
    {
      "category": "Complex Multi-hop",
      "question": "Which players assisted Raphael Veiga's goals?",
      "expected_cypher": "MATCH (p:Player {player_id: 'P003'})<-[scored:SCORED_IN]-(m:Match)<-[assisted:ASSISTED_IN]-(assister:Player) WHERE scored.assisted_by_player_id = assister.player_id RETURN DISTINCT assister.name",
      "expected_result": [
        {"player": "Endrick Felipe"}
      ]
    }
  ],

